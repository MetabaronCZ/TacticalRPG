{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "max-line-length": false,
        "ordered-imports": false,
        "quotemark": [true, "single"],
        "indent": [true, "tabs", 4],
        "trailing-comma": [true, { "esSpecCompliant": true }]
    }
}
